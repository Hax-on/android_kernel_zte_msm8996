/dts-v1/;

#include "msm8996pro.dtsi"
#include "msm-pmi8996.dtsi"
#include "msm8996-mtp.dtsi"
#include "zte-msm8996-v3-pmi8996-ailsa_ii.dtsi"
/ {
	model = "Qualcomm Technologies, Inc. MSM 8996pro + PMI8996 MTP";
	compatible = "qcom,msm8996-mtp", "qcom,msm8996", "qcom,mtp";
	qcom,board-id = <100 0>;
};
&soc {
    zte_misc {
        compatible = "zte-misc";
        zte,fingerprint_gpio_pwr = <&tlmm 85 0x0>;
        zte,fingerprint_gpio_id = <&tlmm 21 0x0>;
        pinctrl-names = "ztemisc_fpid_active";
        pinctrl-0 = <&ztemisc_fpid_active>;
    };
    zte-hall {
        compatible = "zte-hall";
        hall_en{
            label = "hall_en";
            gpios = <&tlmm 124 0x0>;
        };
    };
};
&pmi8994_fg{
    qcom,vbat-estimate-diff-mv = <150>;
};
